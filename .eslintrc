{
	"parser": "@babel/eslint-parser",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"require-jsdoc": "off",
		"quotes": "off",
		"linebreak-style": "off",
		"object-curly-spacing": "off",
		"indent": "off",
		"no-tabs": "off",
		"operator-linebreak": "off"
	},
	"extends": ["eslint:recommended", "google"],
	"parserOptions": {
		"requireConfigFile": false
	}
}
